{
  "storage":{
    "dfs": {
      "type": "file",
      "connection": "file:///",
      "workspaces": {
        "root" : {
          "location": "/",
          "writable": false,
          "allowAccessOutsideWorkspace": false
        },
        "tmp" : {
          "location": "/tmp",
          "writable": true,
          "allowAccessOutsideWorkspace": false
        }
      },
      "formats": {
        "csv" : {
          "type": "text",
          "extensions": [ "csv" ],
          "delimiter": ","
        },
        "parquet" : {
          "type": "parquet"
        },
        "json" : {
          "type": "json",
          "extensions": [ "json" ]
        },
        "csvh" : {
          "type": "text",
          "extensions": [ "csvh" ],
          "delimiter": ",",
          "extractHeader": true
        }
      },
      "enabled" : false
    },

    "cp": {
      "type": "file",
      "connection": "classpath:///",
      "formats": {
        "csv" : {
          "type": "text",
          "extensions": [ "csv" ],
          "delimiter": ","
        },
        "tsv" : {
                  "type": "text",
                  "extensions": [ "tsv" ],
                  "delimiter": "\t"
        },
        "json" : {
          "type": "json",
          "extensions": [ "json" ]
        },
        "parquet" : {
          "type": "parquet"
        },
        "csvh" : {
          "type": "text",
          "extensions": [ "csvh" ],
          "delimiter": ",",
          "extractHeader": true
        }
      },
      "enabled" : true
    }
  }
}
